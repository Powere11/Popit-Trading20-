local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("By GoldenGuys!", "DarkTheme")
local Tab = Window:NewTab("Buy Items!")
local Section = Tab:NewSection("Buy Items!")
Section:NewButton("Buy Jet!", "...", function()
    local args = {
    [1] = "Jet"
    }
    game:GetService("ReplicatedStorage").RemoteEvents.BuyItemCash:FireServer(unpack(args))
end)
Section:NewButton("Buy Rainbow eTruck", "...", function()
    local args = {
    [1] = "Rainbow eTruck"
